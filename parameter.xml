<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
<parameter name="browser" value="chrome"/>
	<parameter name="username" value="admin"></parameter>
	<parameter name="password" value="admin"></parameter>
	<test thread-count="5" name="Test">
		<classes>
			<class name="testscript.Loginpagetest">
				<methods>
					<include
						name="verifyusercanabletologincorrectusernameandpassword"></include>
				</methods>
			</class>
		</classes>
	</test>
	<test name="Hometest">
	 <parameter name="browser" value="chrome"/>
        <parameter name="username" value="admin"/>
        <parameter name="password" value="admin"/>
        <classes>
            <class name="testscript.Hometest">
                <methods>
                  <include name="verifyusercanlogout"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Adminusertest">
	 <parameter name="browser" value="chrome"/>
        <parameter name="username" value="admin"/>
        <parameter name="password" value="admin"/>
        <classes>
            <class name="testscript.Adminusertest">
                <methods>
                  <include name="adminusers"></include>
                </methods>
            </class>
        </classes>
    </test>
     <test name="Managefootertest ">
	 <parameter name="browser" value="chrome"/>
        <parameter name="username" value="admin"/>
        <parameter name="password" value="admin"/>
        <classes>
            <class name="testscript.Managefootertest ">
                <methods>
                  <include name="Managefootertest "></include>
                </methods>
            </class>
        </classes>
    </test>
    
    
</suite>